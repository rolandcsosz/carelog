{
    "name": "carelog-backend",
    "private": true,
    "version": "0.0.0",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "dev": "tsoa spec && tsc && dotenv -e .env.dev -e .env -- node dist/index.js",
        "build": "tsoa spec && tsc",
        "start": "dotenv -e .env.prod -e .env -- node dist/index.js",
        "clean": "rm -rf dist",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,sass}\"",
        "lint": "eslint .",
        "generate:swagger": "tsoa spec",
        "generate:routes": "tsoa routes && node scripts/fixTsoaImports.js",
        "generate:migration": "dotenv -e .env.dev -- prisma migrate dev --name init",
        "generate:client": "dotenv -e .env.dev -- prisma generate",
        "db:deploy": "dotenv -e .env.prod -- prisma migrate deploy",
        "db:reset": "dotenv -e .env.dev -- prisma migrate reset"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@elastic/elasticsearch": "^8.11.0",
        "@google/genai": "^1.28.0",
        "@prisma/client": "^6.18.0",
        "bcrypt": "^5.1.1",
        "cors": "^2.8.5",
        "dotenv": "^16.5.0",
        "express": "^4.21.2",
        "ioredis": "^5.8.2",
        "joi": "^18.0.1",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.14.1",
        "swagger-ui-express": "^5.0.1",
        "ws": "^8.18.3",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.3",
        "@types/joi": "^17.2.2",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/node": "^22.7.4",
        "@types/pg": "^8.10.3",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/ws": "^8.18.1",
        "dotenv-cli": "^10.0.0",
        "eslint": "^9.38.0",
        "google-auth-library": "^10.5.0",
        "prettier": "^3.6.2",
        "prisma": "^6.18.0",
        "ts-node": "^10.9.2",
        "tsoa": "^6.6.0",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.46.2"
    }
}